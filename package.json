{
	"name": "extract-resource-pack",
	"version": "0.1.1",
	"description": "A tool to extract resource pack from Minecraft client",
	"main": "./app/main.js",
	"scripts": {
		"start": "electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"lint": "eslint app",
		"test": "npm run lint"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SavageCore/extract-resource-pack.git"
	},
	"keywords": [
		"minecraft",
		"resource pack",
		"extract"
	],
	"author": "SavageCore",
	"license": "Unlicense",
	"bugs": {
		"url": "https://github.com/SavageCore/extract-resource-pack/issues"
	},
	"homepage": "https://github.com/SavageCore/extract-resource-pack#readme",
	"devDependencies": {
		"babel-plugin-transform-async-to-generator": "^6.22.0",
		"babel-preset-env": "^1.1.8",
		"babel-preset-react": "^6.23.0",
		"electron-prebuilt-compile": "1.4.15",
		"eslint": "^3.12.2",
		"eslint-config-airbnb": "^13.0.0",
		"eslint-plugin-import": "^2.2.0",
		"eslint-plugin-jsx-a11y": "^2.2.3",
		"eslint-plugin-react": "^6.8.0"
	},
	"dependencies": {
		"electron-compile": "^5.1.3",
		"electron-prebuilt-compile": "1.4.15",
		"file-saver": "^1.3.3",
		"jszip": "^3.1.3",
		"walk": "^2.3.9"
	},
	"xo": {
		"rules": {
			"max-nested-callbacks": [
				1,
				10
			],
			"block-scoped-var": 0
		}
	},
	"config": {
		"forge": {
			"make_targets": {
				"win32": [
					"squirrel"
				],
				"darwin": [
					"zip"
				],
				"linux": [
					"deb",
					"rpm"
				]
			},
			"electronPackagerConfig": {},
			"electronWinstallerConfig": {
				"setupExe": "Setup.exe"
			},
			"electronInstallerDebian": {},
			"electronInstallerRedhat": {},
			"github_repository": {
				"owner": "SavageCore",
				"name": "extract-resource-pack",
				"prerelease": true
			},
			"windowsStoreConfig": {
				"packageName": ""
			}
		}
	}
}
