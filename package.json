{
  "name": "extract-resource-pack",
  "version": "0.1.2",
  "description": "A tool to extract resource pack from Minecraft client",
  "main": "./app/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "xo",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SavageCore/extract-resource-pack.git"
  },
  "keywords": [
    "minecraft",
    "resource pack",
    "extract"
  ],
  "author": "SavageCore",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/SavageCore/extract-resource-pack/issues"
  },
  "homepage": "https://github.com/SavageCore/extract-resource-pack#readme",
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.23.0",
    "electron-prebuilt-compile": "1.4.15",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.8.0",
    "xo": "^0.17.1"
  },
  "dependencies": {
    "del": "^3.0.0",
    "electron-compile": "^6.0.0",
    "electron-prebuilt-compile": "1.4.15",
    "file-saver": "^1.3.3",
    "jszip": "^3.1.3",
    "walk": "^2.3.9"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {},
      "electronWinstallerConfig": {
        "setupExe": "Setup.exe"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "SavageCore",
        "name": "extract-resource-pack",
        "prerelease": true
      },
      "windowsStoreConfig": {
        "packageName": ""
      }
    }
  }
}
